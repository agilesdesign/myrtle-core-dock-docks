        {"type": "vcs","url": "https://github.com/agilesdesign/myrtle-core-dock-roles.git"},
        {"type": "vcs","url": "https://github.com/agilesdesign/myrtle-core-dock-users.git"},
        {"type": "vcs","url": "https://github.com/agilesdesign/exceptum.git"}
    ],
    "require": {
        "agilesdesign/flasher": "dev-master",
        "agilesdesign/myrtle-core-dock-configurations": "dev-master",
